import java.io.*;
import java.net.*;
import java.util.ArrayList;
import java.util.Scanner;

public class Yoink {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		ArrayList<String> results = new ArrayList<String>();
		boolean goodInput = false;
		// Get url for bandcamp
		String urlToRead = null;
		System.out.println("Please enter the URL to the album or song from BandCamp.");
		while (!goodInput) {
			urlToRead = sc.nextLine();
			if (urlToRead.contains("bandcamp.com") && urlToRead.contains("https://")) {
				goodInput = true;
			} else
				System.out.println("Please enter a valid album or song url from bandcamp.com");
		}
		URL url;
		HttpURLConnection conn;
		BufferedReader rd;
		String line;
		String html = "";
		try {
			url = new URL(urlToRead);
			conn = (HttpURLConnection) url.openConnection();
			conn.setRequestMethod("GET");
			rd = new BufferedReader(new InputStreamReader(conn.getInputStream()));
			while ((line = rd.readLine()) != null) {
				html += line;
			}
			rd.close();
		} catch (Exception e) {
			e.printStackTrace();
		}

		int startIndex = 0;
		int endIndex = 0;
		while (startIndex > -1 && endIndex > -1) {
			startIndex = html.indexOf("https://t4.bcbits.com");
			endIndex = html.indexOf("&quot;},&quot;track_id");

			try {
				if (!(startIndex == -1) && !(endIndex == -1)) {
					results.add(html.substring(startIndex, endIndex));
					html = html.substring(endIndex + 20);
					startIndex = 0;
					endIndex = 0;
				}
			} catch (Exception e) {
				System.out.println("Something went wrong!");
				break;
			}

		}
		for (int i = 0; i < results.size(); i++) {
			System.out.println(results.get(i));
		}

	}

}
